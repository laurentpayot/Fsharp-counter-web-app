{
    "vite.port": 5000,
    "browse-lite.startUrl": "localhost:5000",
    "cSpell.words": [
        "Elmish",
        "fantomas",
        "Feliz",
        "Ionide",
        "paket",
        "remotedev",
        "Xunit"
    ],
    "testExplorer.hideWhen": "noTests",
    "[fsharp]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "Ionide.Ionide-fsharp",
        "editor.inlayHints.enabled": "offUnlessPressed",
    },
    "FSharp.showExplorerOnStartup": false,
    "FSharp.excludeProjectDirectories": [
		".git",
        ".config",
		".paket",
        ".vscode",
		"bin",
		"dist",
		"node_modules",
		"obj",
		"output",
		"paket-files",
    ],
}
